{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "PspRouter.Trainer": "Debug"
    }
  },
  "PspRouter": {
    "OpenAI": {
      "ApiKey": "your-openai-api-key-here",
      "BaseModel": "gpt-4o-mini",
      "FineTunedModel": "ft:gpt-4o-mini:your-org:your-model:your-id"
    }
  },
  "Training": {
    "Enabled": false,
    "DataPath": "training-data.jsonl",
    "BaseModel": "gpt-4o-mini",
    "Suffix": "psp-router",
    "Hyperparameters": {
      "nEpochs": 3,
      "batchSize": 1,
      "learningRateMultiplier": 1.0,
      "promptLossWeight": 0.01
    },
    "ValidationSplit": 0.1,
    "MaxWaitTimeMinutes": 60
  },
  "Environment": "Development"
}
