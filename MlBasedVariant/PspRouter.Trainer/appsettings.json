{
  "Trainer": {
    "TargetSampleSize": 50000,
    "DateWindowMonths": 6,
    "MaxPerSegment": 50,
    "TargetSuccessRatio": 0.5,
    "ModelOutputPath": "models/psp_routing_model.zip",
    "MaxIterations": 1000,
    "LearningRate": 0.1,
    "NumLeaves": 31,
    "MinDataInLeaf": 20,
    "FeatureFraction": 0.8,
    "BaggingFraction": 0.8,
    "BaggingFreq": 5,
    "ValidationFraction": 0.2,
    "EarlyStoppingRounds": 50,
    "Seed": 42,
    "Objective": "binary",
    "Metric": "binary_logloss"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "PspRouter.Trainer": "Debug"
    }
  },
  "Environment": "Development"
}
