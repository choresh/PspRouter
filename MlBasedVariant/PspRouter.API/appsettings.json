{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ML": {
    "ModelPath": "models/psp_routing_model.zip"
  },
  "PspRouter": {
    "Routing": {
      "Weights": {
        "AuthWeight": 1.0,
        "FeeBpsWeight": 1.0,
        "FixedFeeWeight": 1.0,
        "BusinessBiasWeight": 0.0,
        "BusinessBias": {
          "Adyen": 0.0,
          "Stripe": 0.0,
          "Klarna": 0.0,
          "PayPal": 0.0
        },
        "Supports3DSBonusWhenSCARequired": 0.0,
        "RiskScorePenaltyPerPoint": 0.0,
        "HealthYellowPenalty": 0.0
      },
      "AllowedHealthStatuses": ["green", "yellow"]
    },
    "PspCandidates": {
      "TrustServerCertificate": true,
      "QueryTimeoutSeconds": 30,
      "EnableRetry": true,
      "MaxRetryAttempts": 3,
      "RetryDelayMs": 1000
    }
  }
}